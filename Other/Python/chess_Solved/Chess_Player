#Chess game
#By Daniel Graham



class board:

    def __init__(self):
        self.board = [8][8]
        
class pawn:

    def __init__(self, position):
        self.position = position
        self.firstMove = True

    def move(self, position):
        self.position = position
        self.firstMove = False

    def getPosition(self):
        return self.position

    def isFirstMove(self):
        return self.firstMove


class rook:

    def __init__(self, position):
        self.position = position

    def move(self, position):
        self.position
