     '               ``   -   .        .y\n',                                       
     '                `````/`. `       .y\n',                                        
     '                      :- .       .y\n',                                        
     '                   ``` ./.       .y\n',                                        
     ]
buldgerb = [
     '              `o-        -       .o                       ..\n',               
     '                :o/           `-::///:-.               :++:\n',               
