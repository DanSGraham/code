     '                  h`                               s-\n',                      
    '-///////////////:  h                                :+ .///////////////////`\n',
     '                  d                                //\n',                      

     '                  s-                               h`\n',                      
     ]
bludgerd = [
     '                  `h                              /+\n',                      
